/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=a, b=b, out=tmp0);
    And(a=a, b=b, out=tmp1);
    Not(in=tmp1, out=tmp2);
    And(a=tmp0, b=tmp2, out=out);
}
